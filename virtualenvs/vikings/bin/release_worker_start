#!/home/vikings/github/augur/virtualenvs/vikings/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'release-worker','console_scripts','release_worker_start'
__requires__ = 'release-worker'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('release-worker', 'console_scripts', 'release_worker_start')()
    )

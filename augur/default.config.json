        "github_worker": {
            "port": 56211,
            "switch": 0,
            "workers": 2
        },
        "insight_worker": {
            "port": 56311,
            "switch": 0,
            "workers": 2
        },
        "linux_badge_worker": {
            "port": 56811,
            "switch": 0,
            "workers": 1
        },
        "metric_status_worker": {
            "port": 56711,
            "switch": 0,
            "workers": 1
        },
        "pull_request_worker": {
            "port": 56411,
            "switch": 0,
            "workers": 2
        },
        "repo_info_worker": {
            "port": 56511,
            "switch": 0,
            "workers": 2

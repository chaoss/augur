.loader {
  width: 100%
  text-align: center
  line-height: 10

  margin: 0px 0px 0px 270px;
  position: absolute;
  transform: translateY(-50%);

  border: 16px solid #7d7d7d; /* our red */
  border-top: 16px solid #FF3647; /* our gray */
  border-radius: 50%;
  opacity: 0.5
  width: 120px;
  height: 120px;
  animation: spin 2s linear infinite;
}

@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

@keyframes outro
  0%
    opacity: 1
    filter: grayscale(0%)
  100%
    opacity: 0.2
    filter: grayscale(100%)

@keyframes loader
  0%
    opacity: 0.2
    filter: grayscale(100%)
  50%
    opacity: 0.4
    filter: grayscale(50%)
  100%
    opacity: 0.2
    filter: grayscale(100%)

.intro
  animation: intro 2s

@keyframes intro
  0%
    opacity: 0.2
    filter: grayscale(100%)
  100%
    opacity: 1
    filter: grayscale(0%)


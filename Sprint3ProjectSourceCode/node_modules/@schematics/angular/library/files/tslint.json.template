{
  "extends": "<%= relativePathToWorkspaceRoot %>/tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "<%= camelize(prefix) %>",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "<%= dasherize(prefix) %>",
      "kebab-case"
    ]
  }
}

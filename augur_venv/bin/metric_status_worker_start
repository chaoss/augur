#!/Users/elitanelson/github/augur/augur_venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'metric-status-worker==0.1.0','console_scripts','metric_status_worker_start'
__requires__ = 'metric-status-worker==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('metric-status-worker==0.1.0', 'console_scripts', 'metric_status_worker_start')()
    )

#!/Users/elitanelson/github/augur/augur_venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'linux-badge-worker==0.1.0','console_scripts','linux-badge-worker'
__requires__ = 'linux-badge-worker==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('linux-badge-worker==0.1.0', 'console_scripts', 'linux-badge-worker')()
    )
